generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id        String   @id @default(cuid())
  year      Int      @unique
  theme     String?
  isActive  Boolean  @default(false)
  createdAt DateTime @default(now())
}
